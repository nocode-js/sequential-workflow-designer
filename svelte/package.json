{
	"name": "sequential-workflow-designer-svelte",
	"description": "Svelte wrapper for Sequential Workflow Designer component.",
	"version": "0.34.1",
	"license": "MIT",
	"scripts": {
		"prepare": "cp ../LICENSE LICENSE",
		"start": "vite dev",
		"build": "svelte-kit sync && vite build && svelte-package",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:single": "echo \"No tests yet\"",
		"prettier": "prettier --plugin prettier-plugin-svelte --check ./src",
		"prettier:fix": "prettier --plugin prettier-plugin-svelte --write ./src",
		"eslint": "eslint ./src --ext .ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0",
		"sequential-workflow-designer": "^0.34.1"
	},
	"devDependencies": {
		"sequential-workflow-designer": "^0.34.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@sveltejs/package": "^2.0.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"eslint": "^8.30.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"keywords": [
		"workflow",
		"designer",
		"builder",
		"nocode",
		"lowcode",
		"flow",
		"svelte",
		"sveltejs"
	]
}